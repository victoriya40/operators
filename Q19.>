import java.util.Scanner;
public class Main{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        String result=(a > b && a > c) ? "A is greatest"
                       : (b > a && b > c) ? "B is greatest"
                       : "C is greatest";
        System.out.println(result);
	}
}
